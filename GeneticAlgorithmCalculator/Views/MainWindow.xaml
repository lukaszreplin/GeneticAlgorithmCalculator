<Window x:Class="GeneticAlgorithmCalculator.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="608" Width="811">
    <Grid>
        <Button Content="Process" FontWeight="Bold" FontSize="14" HorizontalAlignment="Left" Margin="674,523,0,0" VerticalAlignment="Top" Width="108" Height="28"/>
        <GroupBox Header="Parameters" HorizontalAlignment="Left" Height="117" Margin="10,10,0,0" VerticalAlignment="Top" Width="772">
            <Grid>
                <Label Content="Range" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                <Label Content="&lt;" HorizontalAlignment="Left" Margin="53,10,0,0" VerticalAlignment="Top"/>
                <TextBox HorizontalAlignment="Left" Height="23" Margin="71,14,0,0" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" Width="65"/>
                <Label Content="," HorizontalAlignment="Left" Margin="136,10,0,0" VerticalAlignment="Top"/>
                <TextBox HorizontalAlignment="Left" Height="23" Margin="149,14,0,0" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" Width="65"/>
                <Label Content="&gt;" HorizontalAlignment="Left" Margin="214,10,0,0" VerticalAlignment="Top"/>
                <Label Content="Precision" HorizontalAlignment="Left" Margin="254,10,0,0" VerticalAlignment="Top"/>
                <ComboBox HorizontalAlignment="Left" Margin="316,14,0,0" VerticalAlignment="Top" Width="120"/>
                <Label Content="Population size" HorizontalAlignment="Left" Margin="452,11,0,0" VerticalAlignment="Top"/>
                <TextBox HorizontalAlignment="Left" Height="23" Margin="547,14,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
                <TextBox HorizontalAlignment="Left" Height="23" Margin="149,53,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="65"/>
                <Label Content="Number of generations" HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top" Width="134"/>

            </Grid>
        </GroupBox>
        <GroupBox Header="Results" HorizontalAlignment="Left" Height="386" Margin="10,132,0,0" VerticalAlignment="Top" Width="772"/>

    </Grid>
</Window>
